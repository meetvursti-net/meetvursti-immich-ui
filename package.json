{
  "name": "@meetvursti-net/immich-monorepo",
  "version": "0.0.1",
  "description": "monorepo for immich and friends",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "pnpm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetvursti-net/meetvursti-immich-ui.git"
  },
  "scripts": {
    "start": "pnpm --filter @immich/ui start",
    "build": "pnpm -r --filter=./packages/* build",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "pnpm lint -- --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "check": "pnpm --filter @immich/svelte-markdown-preprocess build && pnpm -r check",
    "test": "pnpm -r --filter=./packages/* test"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@sveltejs/eslint-config": "^8.3.4",
    "@types/eslint": "^9.6.1",
    "acorn": "latest",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.5",
    "globals": "^17.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "svelte": "^5.42.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.0"
  }
}
